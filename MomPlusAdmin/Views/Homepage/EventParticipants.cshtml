@model IEnumerable<MomPlusAdmin.Domain.Entity.Calendar>

<table class="table">
    <tr>
        <th>
            Start Date
        </th>
        <th>
           End Date
        </th>
        <th>
           Event Title
        </th>
        <th>
          Participants
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.start_date)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.end_date)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Event)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.feedback)
        </td>
    </tr>
}

</table>
